        <AuxValue name="JavaCodeGenerator_InitCodePost" type="java.lang.String" value="sliderTime.addMouseListener(new MouseAdapter() {&#xd;&#xa;            @Override&#xd;&#xa;            public void mousePressed(MouseEvent e) {&#xd;&#xa;                // when the user is holding the tick we want to the pause the song&#xd;&#xa;                player.pauseSong();&#xd;&#xa;            }&#xd;&#xa;&#xd;&#xa;            @Override&#xd;&#xa;            public void mouseReleased(MouseEvent e) {&#xd;&#xa;                JSlider source = (JSlider) e.getSource();&#xd;&#xa;&#xd;&#xa;                int frame = source.getValue();&#xd;&#xa;&#xd;&#xa;                player.setCurrentFrame(frame);&#xd;&#xa;&#xd;&#xa;                player.setCurrentTimeInMilli((int) (frame / (2.08 * player.getCurrentSong().getFrameRatePerMilliseconds())));&#xd;&#xa;&#xd;&#xa;                player.playCurrentSong();&#xd;&#xa;&#xd;&#xa;                }&#xd;&#xa;        });"/>
